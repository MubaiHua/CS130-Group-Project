{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules":{
        "linebreak-style": 0,
        "react/jsx-props-no-spreading": "off",
        "camelcase": "off"
    }
}
